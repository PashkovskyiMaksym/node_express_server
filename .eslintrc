{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    " sourceType": ""
  },
  "rules": {
    "prettier/prettier": "error",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]

  }
}
